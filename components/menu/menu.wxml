<view class="content">
  <t-swiper bindclick="handlerSwiper" current="{{current}}" autoplay="{{autoplay}}" duration="{{duration}}" interval="{{interval}}" list="{{swiperList}}" navigation="{{ { type: 'dots-bar' } }}" />
  <view class="head">
    <!-- <view class="cover">
      <image class="logo" src="https://p2.music.126.net/rNQH185Dp2lcG2yII_Aiwg==/18159534045164927.jpg"></image>
    </view>
    <view class="name"> Welcome to shuke lab</view> -->
  </view>
  <view class="body">
    <view class="module-list">
      <div class="button" bind:tap="handlerPageRouter" wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="{{index}}" data-params="{{ {item, index} }}">
        <image class="icon-menu" src="{{item.image}}"></image>
        <view class="module-name">{{item.name}}</view>
      </div>

    </view>
  </view>
</view>